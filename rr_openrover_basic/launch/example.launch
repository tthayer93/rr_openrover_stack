<launch>

    <!-- OpenRover Driver -->
    <node pkg="rr_openrover_basic" type="openrover_basic_node" name="openrover_basic_node" output="screen">
        <param name="port" value="/dev/rover" />
        <param name="drive_type" value="4wd" />
        <param name="enable_timeout" type="bool" value="true"/>
        <param name="timeout" type="double" value="0.3"/>

        <param name="default_low_speed_mode" type="bool" value="false"/>
        <param name="default_low_speed_mode" type="bool" value="false" />
        <param name="total_weight" type="double" value="20.41"/>
        <param name="slippage_factor" type="double" value="0.610"/>
        <param name="odom_covariance_0" type="double" value="0.01"/>
        <param name="odom_covariance_35" type="double" value="0.03"/>
    </node>

</launch>
